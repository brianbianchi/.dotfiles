syntax on
syntax enable

set tabstop=4
set softtabstop=4
set shiftwidth=4

set backspace=indent,eol,start
set expandtab
set smartindent
set smartcase
set incsearch

set foldlevelstart=99
set colorcolumn=80
set number
set hlsearch
set scrolloff=8
set ruler

set mouse=a
set noswapfile
set background=dark